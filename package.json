{
	"name": "brixms",
	"version": "2.0.0",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist/**/*"
	],
	"scripts": {
		"compile": "tsc",
		"coverage": "nyc -r lcov -e .ts npm run test && open ./coverage/lcov-report/index.html",
		"lint": "eslint . --ext .ts",
		"test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register -r tsconfig-paths/register 'tests/**/*.test.ts'"
	},
	"dependencies": {
		"mathjs": "10.4.0",
		"rxjs": "7.5.5"
	},
	"devDependencies": {
		"@types/chai": "4.3.0",
		"@types/mocha": "9.1.0",
		"@types/node": "17.0.21",
		"@types/sinon-chai": "3.2.8",
		"@typescript-eslint/eslint-plugin": "4.33.0",
		"@typescript-eslint/parser": "4.33.0",
		"chai": "4.3.6",
		"eslint": "7.32.0",
		"mocha": "9.2.1",
		"nyc": "15.1.0",
		"sinon": "13.0.1",
		"sinon-chai": "3.7.0",
		"ts-loader": "9.2.6",
		"ts-node": "10.6.0",
		"tsconfig-paths": "3.12.0",
		"typescript": "4.5.4"
	}
}
